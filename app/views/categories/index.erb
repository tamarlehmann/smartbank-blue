<h2><%= @heading %> </h2>
<% if @results %>
<table>
  <tr>
    <th>Date</th>
    <th>Transaction</th>
    <th>Amount</th>
  </tr>
  <%@results.each do |result| %>
      <tr>
        <td><%= result.date %></td>
        <td><%= result.vendor %></td>
        <td><%= result.amount %></td>
      </tr>
    </div>
  <% end %>
</table>

<% else %>
  <p>You have no transactions to show for this category</p>
<% end %>
